makeCacheMatrix <- function(x = matrix()) 
{

        m <- NULL
        set <- function(y)
        {
                x <<- y
                m <<- NULL
        }
        get <- function() x
        setinv <- function(y) m <<- y  ## create subfunction to set inverse matrix  
        getinv <- function() m         ## create subfunction to get inverse matrix
        list(set = set, get = get,     ## make a list of subfunction
        setinv = setinv,
        getinv = getinv)

}
